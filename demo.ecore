<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="demo" nsURI="http://www.example.org/demo" nsPrefix="demo">
  <eAnnotations source="http://www.eclipse.org/OCL/Import">
    <details key="ecore" value="http://www.eclipse.org/emf/2002/Ecore"/>
  </eAnnotations>
  <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
    <details key="invocationDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"/>
    <details key="settingDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"/>
    <details key="validationDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"/>
  </eAnnotations>
  <eClassifiers xsi:type="ecore:EClass" name="Demo_ProductionProcesses_SystemUnitClassLib">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ProductionProcess" ordered="false"
        upperBound="-1" eType="#//IntermediateProcess" containment="true">
      <eAnnotations source="http://www.eclipse.org/OCL/Collection">
        <details key="nullFree" value="false"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Demo_ShopFloor">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="UniqueProcessOrderConstraint ProcessSequenceConstraint AppropriateTemperature"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="UniqueProcessOrderConstraint" value="&#xA;&#x9;&#x9;&#x9;self.IntermediateProcesses->isUnique(p | p.processSequenceOrder)"/>
      <details key="ProcessSequenceConstraint" value="&#xA;&#x9;&#x9;&#x9;self.IntermediateProcesses->forAll(p1, p2 | p1 &lt;> p2 implies (p1.processSequenceOrder &lt; p2.processSequenceOrder implies p1.processType &lt; p2.processType))"/>
      <details key="AppropriateTemperature" value="&#xA;&#x9;&#x9;&#x9;self.currentTemperature &lt; self.thresholdTemperature"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="currentTemperature" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="thresholdTemperature" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble" defaultValueLiteral="30.0"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="IntermediateProcesses"
        ordered="false" upperBound="-1" eType="#//IntermediateProcess" containment="true">
      <eAnnotations source="http://www.eclipse.org/OCL/Collection">
        <details key="nullFree" value="false"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="IntermediateProcess">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="processType" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="processSequenceOrder" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger"/>
  </eClassifiers>
</ecore:EPackage>
